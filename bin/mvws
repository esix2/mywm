#! /bin/sh
ifi3=false
ifsway=false
if [ ! -z "$(neofetch | grep i3)" ]; then
    ifi3=true
elif [ ! -z "$(neofetch | grep sway)" ]; then
    ifsway=true
else 
    echo "This commands does not work on any other WM than i3 & sway"
fi
if $ifi3; then
    i3-msg "[workspace=$1]" move workspace to output $2
elif $ifsway; then
    swaymsg "Workspace $1; move to Output $2"
#     swaymsg "workspace 1; move to workspace $2"
fi
